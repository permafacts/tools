{
  "name": "permafacts-tools",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "generate:react": "nx generate @nrwl/react:library ${NAME} --directory=react --buildable --importPath=@permafacts-tools/react-${NAME} --publishable --standaloneConfig --no-interactive",
    "generate:svelte": "nx generate @nxext/svelte:library ${NAME} --directory=svelte --buildable --importPath=@permafacts-tools/svelte-${NAME} --publishable --no-interactive",
    "generate:libs": "npm run generate:react && npm run generate:svelte",
    "generate:svelte-storybook": "nx generate @nxext/svelte:storybook-configuration svelte-${NAME} --standaloneConfig --no-interactive",
    "generate:react-storybook": "nx generate @nrwl/react:storybook-configuration react-${NAME} --no-generateCypressSpecs --standaloneConfig --tsConfiguration --no-interactive",
    "generate:storybooks": "npm run generate:svelte-storybook && npm run generate:react-storybook",
    "svelte:run-storybook": "npm run nx -- run svelte-${NAME}:storybook",
    "react:run-storybook": "npm run nx -- run react-${NAME}:storybook",
    "generate:libs-with-storybooks": "npm run generate:libs && npm run generate:storybooks && node ./tools/scripts/setup-storybook.js ${NAME}"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/schematics": "^15.0.0",
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
