{
  "name": "contracts-tickets",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/contracts/tickets/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/contracts/tickets",
        "tsConfig": "packages/contracts/tickets/tsconfig.lib.json",
        "packageJson": "packages/contracts/tickets/package.json",
        "main": "packages/contracts/tickets/src/index.ts",
        "assets": ["packages/contracts/tickets/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/contracts/tickets/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/contracts/tickets/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
